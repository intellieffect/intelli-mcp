{
  "name": "mcp-config-manager-v2",
  "version": "2.0.0",
  "description": "Simple MCP (Model Context Protocol) configuration manager",
  "main": "dist/main.js",
  "author": "Claude Code",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "mcp",
    "config",
    "manager"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "webpack --config webpack.main.config.ts --mode development --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.ts --mode development",
    "build": "npm run clean && npm run build:main && npm run build:renderer",
    "build:main": "webpack --config webpack.main.config.ts --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.ts --mode production",
    "clean": "rimraf dist release",
    "start": "cross-env NODE_ENV=production electron .",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "package": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "rimraf": "^5.0.7",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.21",
    "@mui/material": "^5.15.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "build": {
    "appId": "com.claude-code.mcp-config-manager-v2",
    "productName": "MCP Config Manager v2",
    "directories": {
      "output": "release",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "darkModeSupport": true
    },
    "win": {
      "target": "nsis",
      "publisherName": "Claude Code"
    },
    "linux": {
      "target": "AppImage",
      "category": "Development"
    }
  }
}